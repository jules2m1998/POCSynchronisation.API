            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SynchronisationRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SynchronisationRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
