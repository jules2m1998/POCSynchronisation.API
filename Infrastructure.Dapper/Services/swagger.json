{
  "openapi": "3.0.1",
  "info": {
    "title": "Api",
    "version": "1.0"
  },
  "paths": {
    "/api/Synchronisation": {
      "post": {
        "tags": [
          "Synchronisation"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SynchronisationRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SynchronisationRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SynchronisationRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SynchronisationResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "get": {
        "tags": [
          "Synchronisation"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/SynchronisedStoredEventDto"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/api/Synchronisation/retrieve": {
      "get": {
        "tags": [
          "Synchronisation"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/StoredRetieverDto"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "EventType": {
        "enum": [
          0,
          1,
          2,
          3
        ],
        "type": "integer",
        "format": "int32"
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": {}
      },
      "StoredRetieverDto": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "dataJson": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SynchronisationRequest": {
        "type": "object",
        "properties": {
          "events": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SynchronisedStoredEventDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SynchronisationResponse": {
        "type": "object",
        "properties": {
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SynchronisedStoredEventDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SynchronisedStoredEventDto": {
        "type": "object",
        "properties": {
          "eventType": {
            "type": "string",
            "nullable": true
          },
          "dataType": {
            "type": "string",
            "nullable": true
          },
          "dataJson": {
            "type": "string",
            "nullable": true
          },
          "conflictWithJson": {
            "type": "string",
            "nullable": true
          },
          "eventId": {
            "type": "string",
            "format": "uuid"
          },
          "mobileEventId": {
            "type": "string",
            "format": "uuid"
          },
          "elementId": {
            "type": "string",
            "format": "uuid"
          },
          "eventStatus": {
            "$ref": "#/components/schemas/EventType"
          },
          "emitedOn": {
            "type": "string",
            "format": "date-time"
          },
          "savedOn": {
            "type": "string",
            "format": "date-time"
          },
          "lastSyncEvent": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      }
    }
  }
}